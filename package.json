{
  "name": "thaya.api",
  "version": "1.0.0",
  "description": "Thaya API",
  "author": "Valmir Jr.",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "GOOGLE_APPLICATION_CREDENTIALS=\"./gcp-credentials.json\" nest start",
    "dev": "GOOGLE_APPLICATION_CREDENTIALS=\"./gcp-credentials.json\" nest start --watch",
    "debug": "GOOGLE_APPLICATION_CREDENTIALS=\"./gcp-credentials.json\" nest start --debug --watch",
    "prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "redeploy:assistants": "GOOGLE_APPLICATION_CREDENTIALS=\"./gcp-credentials.json\" npx tsx scripts/ResetAssistants.ts",
    "redeploy:db": "npx tsx scripts/ResetDB.ts",
    "redeploy:all": "npm run redeploy:db && npm run redeploy:assistants",
    "redeploy:prod:assistants": "ENVIRONMENT=prod GOOGLE_APPLICATION_CREDENTIALS=\"./gcp-credentials.json\" npx tsx scripts/ResetAssistants.ts",
    "redeploy:prod:db": "ENVIRONMENT=prod npx tsx scripts/ResetDB.ts",
    "redeploy:prod:all": "ENVIRONMENT=prod npm run redeploy:prod:db && npm run redeploy:prod:assistants"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@google-cloud/storage": "^7.15.0",
    "@nestjs/common": "^11.1.1",
    "@nestjs/core": "^11.0.3",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^11.0.3",
    "@nestjs/platform-socket.io": "^11.0.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/websockets": "^11.0.3",
    "axios": "^1.7.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.3.0",
    "googleapis": "^144.0.0",
    "moment": "^2.30.1",
    "mongoose": "^8.9.5",
    "openai": "^4.79.1",
    "swagger-themes": "^1.4.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@google-cloud/secret-manager": "^6.1.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.3",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "mongodb": "^6.16.0",
    "prettier": "3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".(spec|test).ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "collectCoverage": true,
    "verbose": true
  }
}